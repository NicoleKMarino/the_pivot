<div class="page-content">
  <h1>Current Applications</h1>
  
  <div class="jobs-list">
  <% @bucket_jobs.each do |job| %>
    <h2><%=job.title %></h2>
    <p>
      <%= job.summary %>
    </p>
    <%= button_to "Remove this Application", bucket_job_path(job), method: :delete %>
  <% end %>
  </div>
</div>
  