<div class="page-content">

<h2>Profile for <%= current_user.username %></h2>
  <div class="well table-responsive col-md-6">
    <table class="table">
      <tr>
        <th>E-mail</th>
        <td><%= current_user.email %></td>
      </tr>
      <tr>
        <th>Date Registered</th>
        <td><%= current_user.date_registered %></td>
      </tr>
        <tr>
          <th>Address</th>
          <td><%= current_user.address %>,
          <%= current_user.city %>,
          <%= current_user.state %>,
          <%= current_user.zip_code %></td>
        </tr>
    </table>
  </div>
</div>
